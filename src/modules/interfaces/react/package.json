{
  "name": "cyber-autoagent-cli",
  "version": "0.1.3",
  "description": "Cyber-AutoAgent Professional CLI with React + Ink",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "cyber-react": "dist/index.js",
    "cyber-native": "dist/index-native.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.tsx",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest src/tests/integration.test.ts --verbose",
    "test:all": "npm run build && npm run test:integration",
    "validate": "npm run build && npm run test:all"
  },
  "dependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/yaml": "^1.9.6",
    "ansi-escapes": "^7.0.0",
    "chokidar": "^4.0.3",
    "commander": "^11.1.0",
    "date-fns": "^4.1.0",
    "dockerode": "^4.0.2",
    "glob": "^10.3.10",
    "ink": "^4.4.1",
    "ink-gradient": "^3.0.0",
    "ink-link": "^3.0.0",
    "ink-progress-bar": "^3.0.0",
    "ink-select-input": "^5.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^5.0.1",
    "js-yaml": "^4.1.0",
    "meow": "^12.1.1",
    "node-pty": "^1.0.0",
    "react": "^18.2.0",
    "strip-ansi": "^7.1.0",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/dockerode": "^3.3.5",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.9",
    "@types/react": "^18.2.0",
    "chalk": "^5.4.1",
    "ink-testing-library": "^4.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.9.2"
  }
}
